<%- include('basic/header'); %>

    <form action="/instituicao" method="post">
        <label for="nome">Nome Instituição</label>
        <input type="text" id="nome" name="nome" placeholder="Nome da instituição">
        <label for="responsavel">Contato da Instituição</label>
        <input type="text" id="responsavel" name="responsavel" placeholder="Responsável pela Instituição">
        <label for="email">E-mail</label>
        <input type="email" id="email" name="email" placeholder="responsavel@instituica.com.br">
        <label for="telefone">Telefone</label>
        <input type="tel" id="telefone" name="telefone">
        <button type="submit">Cadastrar</button>
    </form>

<%- include('basic/footer'); %>